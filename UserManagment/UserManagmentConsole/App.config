<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <section name="linq2db" type="LinqToDB.Configuration.LinqToDBSection, linq2db" requirePermission="false" />
    </configSections>

    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>


    <linq2db defaultConfiguration="FirstDbSQLite" />

    <connectionStrings>
        <add name="FirstDbSQLite"
             connectionString="Data Source=..\..\..\..\DataBase\first_db.sqlite"
             providerName="SQLite" />
    </connectionStrings>
</configuration>